@using JiuBa.Models
@model  List<Model>
@if (Model.Count() > 0)
{
	foreach (var x in Model)
	{
	<a href="/UserPost/GetPostALLByModelId?id=@x.ModelId">
		<dl class="text-center user_model_box" onclick="postmodel(@x.ModelId)">
			<dt><img src="~/img/@x.ModelImg" class="user_model_img" /></dt>
			<dd><label>@x.ModelName</label></dd>
		</dl>
	</a>
	}
}


